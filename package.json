{
  "name": "netlify-template-jekyll",
  "version": "",
  "description": "A Jekyll Netlify Template built using PatternFly",
  "main": "index.js",
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "build": "gulp build",
    "jekyll": "bundle exec jekyll build",
    "install": "bundle install",
    "deployment": "gulp deploy",
    "development": "gulp development",
    "changelog": "gren release -P --override && gren changelog --override",
    "release": "scripts/./release.sh prerelease"
  },
  "sass": {
    "includePaths": [
      "./node_modules"
    ]
  },
  "repository": "https://github.com/mindreeper2420/netlify-template-jekyll.git",
  "url": "https://mindreeper2420-netlify-template-jekyll.netlify.com",
  "author": "Adam J. Jolicoeur",
  "license": "MIT",
  "devDependencies": {
    "@patternfly/patternfly": "2.61.0",
    "@skpm/child_process": "0.4.1",
    "autoprefixer": "9.7.3",
    "browser-sync": "2.26.7",
    "cssnano": "4.1.10",
    "del": "5.1.0",
    "flat-map": "1.0.0",
    "github-release-notes": "0.17.1",
    "gulp": "4.0.2",
    "gulp-clean": "0.4.0",
    "gulp-exec": "4.0.0",
    "gulp-header": "2.0.9",
    "gulp-imagemin": "6.2.0",
    "gulp-newer": "1.4.0",
    "gulp-noop": "1.0.0",
    "gulp-postcss": "8.0.0",
    "gulp-sass": "4.0.2",
    "gulp-scale-images": "2.0.1",
    "gulp-size": "3.0.0",
    "gulp-sourcemaps": "2.6.5",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-pngquant": "8.0.0"
  }
}
